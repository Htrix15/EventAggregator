{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MessageBroker": {
    "Settings": {
      "BootstrapServers": "localhost:9092",
      "Client": "WebApi"
    },
    "Producers": {
      "StartShowAggregation": {
        "EnableIdempotence": false,
        "MessageSendMaxRetries": 3,
        "RetryBackoffMs": 100,
        "CheckSending": true,
        "FlushTimeoutMs": 10000
      }
    }
  }
}
